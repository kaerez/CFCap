{
  "name": "cfcap",
  "main": "src/index.js",
  "compatibility_date": "2024-09-23",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "assets": {
    "directory": "./public",
    "binding": "ASSETS"
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "cfcap-db",
      "database_id": "PLACEHOLDER_DB_ID"
    }
  ],
  "observability": {
    "enabled": true
  }
}
