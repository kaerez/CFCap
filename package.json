{
  "name": "cfcap",
  "version": "1.0.0",
  "description": "CFCap - Cloudflare Worker for @cap.js/server",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run build && wrangler dev",
    "build": "npm run setup && rm -rf dist && mkdir -p dist && cp -r demo dist/ && cp -r widget dist/",
    "postinstall": "npm run build",
    "setup": "node scripts/setup.js",
    "deploy": "npm run build && wrangler deploy",
    "cf-typegen": "wrangler types"
  },
  "author": "Erez Kalman",
  "license": "Commercial",
  "dependencies": {
    "@cap.js/server": "4.0.5"
  },
  "devDependencies": {
    "wrangler": "4.59.1"
  }
}
